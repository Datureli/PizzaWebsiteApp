ANL,EAMK,IANL,EAMK,CALH,YAAA,CAIM,KAJN,EAIM,IAJN,EAIM,CAHJ,YAAA,CAAwB,IAAxB,EAAwB,IAAxB,EAAwB,gBAAA,CAAjB,IAAI,CAAC,IAAY,CAAxB,EAAgB;AAAA;AAAhB,KAGI,EAFJ,YAAA,CAAwB,GAAxB,EAAwB,IAAxB,EAAG,MAAC,gBAAA,CAAG,IAAI,CAAC,KAAR,CAAJ,EAAiB;AAAA;AAAjB,KAEI,EADJ,YAAA,CAAqD,QAArD,EAAqD;AAA5C,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,SAAA,CAAU,IAAV,CAAF;AAAA;AAAuC,KAArD,EAAiC,aAAjC,EAA4C;AAAA;AAA5C,MAA4C,CAAA,SAAA,CAA5C,CACI,CAJN,CAKG,CANL,C;GAMK,CANL,E;;AAAA,GAOG,EARL,CASI,CAXN,E;;AAAA,G,aAA2B,KAAA,CAAA,Q,GAuBvB,E,gBAXJ,YAAA,CAUM,KAVN,EAAA,UAAA,EAUM,CATJ,WASI,G,kBARJ,YAAA,CAA+I,SAA/I,EAA+I,IAA/I,EAA+I,WAAA,CAA5H,KAAA,CAAA,KAA4H,EAAvH,UAAb,IAAa,EAAT;yBAAf,YAAA,CAA+I,IAA/I,EAA+I;AAApH,MAAA,GAAG,EAAE;AAA+G,KAA/I,EAAoC,CAAE,YAAA,CAAoC,QAApC,EAAoC,IAApC,EAAoC,gBAAA,CAAzB,IAAI,CAAC,QAAoB,CAApC,EAAwB;AAAA;AAAxB,KAAF,E,iBAAsC,QAAG,gBAAA,CAAG,IAAI,CAAC,IAAR,C,EAAY;AAAA;KAArD,EAAwD,YAAA,CAA8C,MAA9C,EAA8C,IAA9C,EAAM,MAAC,gBAAA,CAAG,IAAI,CAAC,KAAL,GAAa,IAAI,CAAC,QAArB,CAAP,EAAoC;AAAA;AAApC,KAAxD,CAApC,C;GAA+I,CAA/I,E;;AAAA,GAQI,GAPJ,WAOI,EANJ,YAAA,CAKM,KALN,EAAA,WAAA,EAKM,CAJJ,YAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFJ,YAAA,CAAmD,IAAnD,EAAmD,IAAnD,EAAmD,C,WAAA,EAAxC,YAAA,CAAmC,MAAnC,EAAmC,IAAnC,EAAmC,gBAAA,CAA1B,QAAA,CAAA,KAAA,GAAQ,IAAA,CAAA,QAAkB,CAAnC,EAAyB;AAAA;AAAzB,GAAwC,CAAnD,CAEI,EADJ,WACI,CAHN,CAII,CALN,CAMI,CAVN,E;;AAAA,G,YAA8B,KAAA,CAAA,Q,GAW1B,CAxBN,CAyBI,CA/CN,C","sourcesContent":["<template>\r\n<div>\r\n <div class=\"header\">\r\n    <h2>Comics</h2>\r\n    <div>\r\n      <button @click=\"showCart = !showCart\" v-show=\"!verified\">\r\n        {{ items.length + (items.length > 1 || items.length === 0 ? \" items\" : \" item\") }}\r\n      </button>\r\n    </div>\r\n  </div>\r\n  <div class=\"cart\" v-show=\"showCart\">\r\n    <div v-show=\"items.length > 0\">\r\n      <ul>\r\n        <li v-for=\"item in items\" transition=\"fade\" >\r\n          <p><strong>{{ item.quantity }}</strong> - {{ item.name }} <i class=\"fa fa-trash\" @click=\"removeFromCart(item)\"></i></p>\r\n        </li>\r\n      </ul>\r\n      <div><button @click=\"verified = true, showCart = false\">Check out</button></div>\r\n    </div>\r\n    <div v-show=\"items.length === 0\">\r\n      <p>Your cart is empty!</p>\r\n    </div>\r\n  </div>\r\n  <div class=\"container\">\r\n    <div class=\"shop\" v-show=\"!verified\">\r\n      <h3>New Arrivals</h3>\r\n      <ul>\r\n        <li v-for=\"item in shop\" >\r\n          <div>\r\n            <h5>{{ item.name }}</h5>\r\n            <p>${{ item.price }}</p>\r\n            <button @click=\"addToCart(item)\">Add to cart</button>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"checkout\" v-show=\"verified\">\r\n      <h3>Your Cart</h3>\r\n      <h5 v-for=\"item in items\" :key=\"item\"><strong>{{ item.quantity }}</strong> - {{ item.name }}<span>${{ item.price * item.quantity }}</span></h5>\r\n      <hr />\r\n      <div class=\"row\">\r\n        <div class=\"u-pull-right\">\r\n          <h5>Total: <span>{{ total | currency }}</span></h5>\r\n          <button>Looks Good</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n    items: [],\r\n    shop: shop,\r\n    showCart: false,\r\n    verified: false,\r\n    quantity: 1,\r\n    shop: [\r\n  {\r\n    name: \"One-Punch Man, Vol. 5\",\r\n    price: 10.02,\r\n    quantity: 0\r\n  },\r\n  {\r\n    name: \"Gantz Omnibus, Vol. 1\",\r\n    price: 22.98,\r\n    quantity: 0\r\n  },\r\n  {\r\n    name: \"Dragon Ball Freeza Arc, Vol. 1\",\r\n    price: 15.98,\r\n    quantity: 0\r\n  },\r\n  {\r\n    name: \"Uzumaki: Spiral into Horror, Vol. 1\",\r\n    price: 4.84,\r\n    quantity: 0\r\n  },\r\n  {\r\n    name: \"Superman: Red Son (New Edition)\",\r\n    price: 10.93,\r\n    quantity: 0\r\n  }\r\n]\r\n    }\r\n  },\r\n  computed: {\r\n    total() {\r\n      var total = 0;\r\n      for(var i = 0; i < this.items.length; i++) {\r\n        total += this.items[i].price;\r\n      }\r\n      return total;\r\n    }\r\n  },\r\n  methods: {\r\n    addToCart(item) {\r\n      item.quantity += 1;\r\n      this.items.push(item);\r\n    },\r\n    removeFromCart(item) {\r\n      item.quantity -= 1;\r\n      this.items.splice(this.items.indexOf(item), 1);\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scl>\r\nbody {\r\n  height: 100%;\r\n  background: linear-gradient(to top, #FF6B83, lighten(#FF6B83, 15%)) no-repeat fixed;\r\n}\r\n\r\n#app {\r\n  font-family: 'Roboto Slab', serif;\r\n}\r\n\r\n.header  {\r\n  height: 80px;\r\n  h2 {\r\n    color: #fff;\r\n    font-family: 'Rock Salt';\r\n    float: left;\r\n    font-weight: bolder;\r\n    margin: 10px 20px;\r\n  }\r\n  button {\r\n    border: 0;\r\n    background: #FFDBE0;\r\n    transition: all .1s ease-out;\r\n    &:hover {\r\n      background: darken(#FFDBE0, 2%);\r\n    }\r\n  }\r\n  div {\r\n    float: right;\r\n    display: inline;\r\n    margin: 10px;\r\n  }\r\n}\r\n\r\nli {\r\n  list-style: none;\r\n}\r\n\r\n.fa {\r\n  cursor: pointer;\r\n  margin-left: 5px;\r\n}\r\n\r\n.fade-transition {\r\n  transition: all 0.2s ease-out;\r\n}\r\n\r\n.fade-enter, .fade-leave {\r\n  opacity: 0;\r\n}\r\n\r\n.cart {\r\n  > div {\r\n    z-index: 100;\r\n    background: #fff;\r\n    padding: 20px 30px;\r\n    position: absolute;\r\n    right: 30px;\r\n    box-shadow: 2px 2px 6px 0 rgba(0, 0, 0, 0.3);\r\n    div {\r\n      text-align: center;\r\n    }\r\n  }\r\n  ul, li, p {\r\n    margin-bottom: 0;\r\n  }\r\n  button {\r\n    margin: 20px 0 10px;\r\n    text-transform: uppercase;\r\n    text-decoration: none;\r\n    font-weight: bold;\r\n    letter-spacing: 2px;\r\n  }\r\n  input {\r\n    width: 30px;\r\n  }\r\n}\r\n\r\n.shop {\r\n  h3 {\r\n    position: absolute;\r\n    top: -85px;\r\n    left: -30px;\r\n    color: rgba(255, 255, 255, 0.3);\r\n    font-family: sans-serif;\r\n    font-size: 4em;\r\n    font-weight: bold;\r\n    letter-spacing: -2px;\r\n  }\r\n  ul {\r\n    margin-top: 90px;\r\n  }\r\n  li div {\r\n    padding: 30px;\r\n    background: #fff;\r\n    margin-bottom: 20px;\r\n    box-shadow: 1px 1px 6px 0 rgba(0, 0, 0, 0.3);\r\n  }\r\n  button {\r\n    color: white;\r\n    border: 0;\r\n    background: #00ADB5;\r\n    margin: 0;\r\n    transition: all .1s ease-out;\r\n    &:hover {\r\n      background: darken(#00ADB5, 5%)\r\n    }\r\n  }\r\n}\r\n\r\n.checkout {\r\n  background: #fff;\r\n  box-shadow: 1px 1px 6px 0 rgba(0, 0, 0, 0.3);\r\n  margin-top: 90px;\r\n  padding: 50px 60px;\r\n  h3 {\r\n    position: absolute;\r\n    top: -85px;\r\n    left: -30px;\r\n    color: rgba(255, 255, 255, 0.3);\r\n    font-family: sans-serif;\r\n    font-size: 4em;\r\n    font-weight: bold;\r\n    letter-spacing: -2px;\r\n  }\r\n  span {\r\n    float: right;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 630px) {\r\n  .checkout {\r\n    padding: 30px 40px;\r\n    h5 {\r\n      font-size: 1.2em;\r\n    }\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 550px) {\r\n  .shop h3, .checkout h3 {\r\n    font-size: 3.3em;\r\n    top: -70px;\r\n    left: 0px;\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}