onback\r\n      </router-link></button>\r\n    </header>\r\n    <div class=\"pizzaParametrs\">\r\n      <h2>Nr</h2>\r\n      <h2>Name</h2>\r\n          <h2>ingredients</h2>\r\n      <h2>Ilość</h2>\r\n    </div>\r\n<div v-for=\"(selected,value) in cart\" :key=\"value\">\r\n  {{ selected}}\r\n\r\n\r\n</div>\r\n    <tr class=\"addedPizzaInformation\" v-for=\"(menu, index) in cart\" :key=\"index\">\r\n      <td>{{ index + 1}}</td>\r\n      <td>{{ menu.quantity }} x {{ menu.pizzaname }}</td>\r\n      <td style=\"display: grid;\">{{ menu.ingredients }}</td>\r\n    \r\n    <button class=\"minusbutton\" v-show=\"menu.quantity > 0\" @click=\"removeFromCart()\">\r\n      X\r\n    </button>\r\n      </tr>\r\n   <router-link :to=\"{ name: 'Cart'}\">Next</router-link>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\n\r\nexport default {\r\n\r\n\r\n  props: {\r\n    cart: Array,\r\n    optionss: Array,\r\n    newValue: String,\r\n    selected: Object,\r\n  },\r\n\r\n\r\n\r\n  methods: {\r\n    Selectt() {\r\n     let Misio = null\r\n      if(selected = 421) {\r\n        Misio = menu.pizzaname\r\n      }\r\n      return Misio\r\n    },\r\n\r\n    back() {\r\n      this.$router.go(-1);\r\n    },\r\n\r\n    removeItemFromCart(product) {\r\n      this.$emit(\"removeItemFromCart\", product);\r\n    },\r\n    \r\n  },\r\n  \r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import url(\"https://use.fontawesome.com/releases/v5.8.2/css/all.css\");\r\n\r\n.container {\r\n  max-width: 920px;\r\n  height: 500px;\r\n  margin: 40px auto;\r\n  background: #555;\r\n  border-radius: 15px;\r\n  box-shadow: 3px 3px 0 black;\r\n\r\n  .menu {\r\n    display: flex;\r\n  }\r\n}\r\nheader {\r\n  width: 100%;\r\n  height: 60px;\r\n  background: orange;\r\n  border-radius: 5px;\r\n  display: flex;\r\n  padding: 10px;\r\n\r\n  h1 {\r\n    width: 100%;\r\n    text-align: left;\r\n    margin-left: 60px;\r\n  }\r\n\r\n  .psize {\r\n    width: 100%;\r\n    display: flex;\r\n    text-align: center;\r\n    justify-content: center;\r\n    padding: 10px;\r\n\r\n    p {\r\n      margin-left: 17px;\r\n      padding: 0;\r\n    }\r\n  }\r\n}\r\n.pizzaParametrs {\r\n  display: flex;\r\n\r\n  h2 {\r\n    margin: 10px 40px\r\n  }\r\n}\r\n.addedPizzaInformation {\r\n  display: flex;\r\n\r\n    td {\r\n    margin: 10px 40px;\r\n      font-size: 30px;\r\n  color: #dddddd;\r\n  padding: 5px;\r\n  }\r\n}\r\ni {\r\n  pointer-events: none;\r\n}\r\n\r\nh2 {\r\n  font-size: 30px;\r\n  color: #dddddd;\r\n  padding: 5px;\r\n  margin-left: 15px;\r\n}\r\na {\r\n  text-decoration: none;\r\n}\r\np {\r\n  font-size: 20px;\r\n  padding: 0 20px 5px;\r\n  color: rgb(184, 169, 169);\r\n}\r\n\r\n.pizzasize {\r\n  display: flex;\r\n\r\n  p {\r\n    margin-top: 40px;\r\n  }\r\n}\r\n\r\n.pizzaname {\r\n  display: flex;\r\n  margin: 16px 45px 0;\r\n  max-height: 80px;\r\n  line-height: 30px;\r\n}\r\n.pizzaname2 {\r\n  width: 500px;\r\n}\r\nbutton {\r\n  width: 95px;\r\n  height: 40px;\r\n  margin: 32px 20px;\r\n  line-height: 25px;\r\n  font-size: 13px;\r\n  background: orange;\r\n  text-align: center;\r\n  justify-content: center;\r\n  font-weight: 600;\r\n\r\n  &:hover {\r\n    background: orangered;\r\n    cursor: pointer;\r\n  }\r\n}\r\n\r\n.minusbutton {\r\n  text-align: center;\r\n  justify-content: center;\r\n  background: none;\r\n  width: 30px;\r\n  height: 30px;\r\n  color: white;\r\n  border-radius: 50%;\r\n  background: red;\r\n  margin: 37px auto;\r\n  padding: 0;\r\n\r\n  &:hover {\r\n    background: #ff0000;\r\n  }\r\n}\r\ntr {\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}