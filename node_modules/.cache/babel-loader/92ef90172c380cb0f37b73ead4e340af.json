              ingredients: 'tomato-souce,ham,garlic,tomato',\r\n                    cm30price: 21,\r\n                    cm40price: 28,\r\n                    cm50price: 41,\r\n                    id: 2421,\r\n                    quantity: 0\r\n                },\r\n                      {   \r\n                    pizzaname: 'Diablo',\r\n                    ingredients: 'tomato-souce,paprika,onion,chillli',\r\n                    cm30price: 24,\r\n                    cm40price: 30,\r\n                    cm50price: 39,\r\n                    id: 3321,\r\n                    quantity: 0\r\n                },\r\n                      {   \r\n                    pizzaname: 'Capriciossa',\r\n                    ingredients: 'tomato-souce,paprika,onion,mushrooms',\r\n                    cm30price: 21,\r\n                    cm40price: 27,\r\n                    cm50price: 37,\r\n                    id: 4441,\r\n                    quantity: 0\r\n                },\r\n            ],\r\n            options: [\r\n                {   \r\n                    value: '30cm',\r\n                    id: 421\r\n                },\r\n                  {\r\n                    value: '40cm',\r\n                    id: 422\r\n                },\r\n                  {\r\n                    value: '50cm',\r\n                    id: 423\r\n                },\r\n            ]\r\n        }\r\n    },\r\n    methods: {\r\n    \r\n     addItemToCart(menu) {\r\n      this.$emit(\"clicked\", menu);\r\n\r\n    },\r\n        addValueToCart(selected) {\r\n      this.$emit(\"selected\", selected);\r\n\r\n    },\r\n    emitModel() {\r\n        this.$emit('update:modelValue', $event.target.value)\r\n    }\r\n},\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  @import url('https://use.fontawesome.com/releases/v5.8.2/css/all.css');\r\n\r\n.container {\r\n      max-width: 920px;\r\n      height: 500px;\r\n      margin: 40px auto;\r\n      background: #555;\r\n      border-radius: 15px;