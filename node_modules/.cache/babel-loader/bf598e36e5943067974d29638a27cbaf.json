      <option value=\"\" disabled selected>Add to Cart</option>\r\n                <option  @change=\"addItemToCart(menu)\">30cm</option>\r\n                <option value=\"pizz\" @click=\"addItemToCart(menu.cm40price)\">40cm</option>\r\n                <option value=\"31\" @click=\"addItemToCart(menu.cm50price)\">50cm</option>   \r\n                 </select>\r\n            </div> \r\n         \r\n           \r\n          <button class=\"minusbutton\" v-show=\"cart > 0 \" v-on:click=\"removeFromCart()\">X</button>\r\n      </div>\r\n  </div>\r\n    \r\n  </div>\r\n  <div>\r\n  </div>\r\n</div>\r\n</template>\r\n<script>\r\nimport BaseSelect from '../../../vue-forms/src/components/BaseSelect.vue';\r\nexport default {\r\n  components: { BaseSelect },\r\n    name: 'Menu',\r\n    data() {\r\n        return {\r\n            \r\n            selected: '',\r\n            cart: [],\r\n            menus: [\r\n                {   \r\n                    pizzaname: 'Margaritta',\r\n                    ingredients: 'tomato-souce,paprika,onion',\r\n                    cm30price: 19,\r\n                    cm40price: 26,\r\n                    cm50price: 39,\r\n                    id: 1321,\r\n                    quantity: 0,\r\n                },\r\n                      {   \r\n                    pizzaname: 'Matador',\r\n                    ingredients: 'tomato-souce,ham,garlic,tomato',\r\n                    cm30price: 21,\r\n                    cm40price: 28,\r\n                    cm50price: 41,\r\n                    id: 2421,\r\n                    quantity: 0\r\n                },\r\n                      {   \r\n                    pizzaname: 'Diablo',\r\n                    ingredients: 'tomato-souce,paprika,onion,chillli',\r\n                    cm30price: 24,\r\n                    cm40price: 30,\r\n                    cm50price: 39,\r\n                    id: 3321,\r\n                    quantity: 0\r\n                },\r\n                      {   \r\n                    pizzaname: 'Capriciossa',\r\n                    ingredients: 'tomato-souce,paprika,onion,mushrooms',\r\n                    cm30price: 21,\r\n                    cm40price: 27,\r\n                    cm50price: 37,\r\n                    id: 4441,\r\n                    quantity: 0\r\n                },\r\n            ]\r\n        }\r\n    },\r\n    methods: {\r\n     addItemToCart(menu) {\r\n      this.$emit(\"clicked\", menu);\r\n    },\r\n}\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  @import url('https://use.fontawesome.com/releases/v5.8.2/css/all.css');\r\n\r\n.container {\r\n      max-width: 920px;\r\n      height: 500px;\r\n      margin: 40px auto;\r\n      background: #555;\r\n      border-radius: 15px;\r\n      box-shadow: 3px 3px 0 black;\r\n    \r\n   .menu {\r\n      display: flex;\r\n}\r\n}\r\n.buttonSelect {\r\n    display: flex;\r\n}\r\nselect {\r\n    z-index: 10;\r\n\r\n}\r\n.selectSize {\r\n    width: 20px;\r\n    height: 20px;\r\n    background: none;\r\n    border: none;\r\n}\r\n   header {\r\n        width: 100%;\r\n        height: 60px;\r\n        background: orange;\r\n        border-radius: 5px;\r\n        display: flex;\r\n        padding: 10px;\r\n     \r\n        h1 {\r\n            width: 100%;\r\n            text-align: left;\r\n            margin-left: 60px;\r\n        }\r\n\r\n        .psize {\r\n            width: 100%;\r\n            display: flex; \r\n            text-align: center;\r\n            justify-content: center;\r\n            padding: 10px;\r\n            margin-right: 90px;\r\n           \r\n\r\n           p {\r\n               margin-left: 17px;\r\n               padding: 0;\r\n\r\n           }\r\n        }\r\n    }\r\n    i {\r\n       pointer-events: none; \r\n    }\r\n\r\nh2 {\r\n    font-size: 30px;\r\n    color: #dddddd;\r\n    padding: 5px;\r\n    margin-left: 15px;\r\n}\r\na {\r\n    text-decoration: none;\r\n}\r\np {\r\n    font-size: 20px;\r\n    padding:0 20px 5px;\r\n    color: rgb(184, 169, 169);\r\n}\r\n\r\n.pizzasize {\r\n    display: flex;\r\n\r\n    p {\r\n        margin-top: 40px\r\n    }\r\n}\r\n\r\n.pizzaname {\r\n    display: flex;\r\n    margin: 16px 45px 0;\r\n    max-height: 80px;\r\n    line-height: 30px;\r\n}\r\n.pizzaname2 {\r\n    width: 500px;\r\n}\r\nbutton {\r\n    width: 115px;\r\n    height: 40px;\r\n    margin: 32px 20px;\r\n    line-height: 25px;\r\n    font-size: 13px;\r\n    background: orange;\r\n    text-align: center;\r\n    justify-content: center;\r\n    font-weight: 600;\r\n\r\n    &:hover {\r\n        background: orangered;\r\n        cursor: pointer;\r\n    }\r\n}\r\n\r\n.minusbutton {\r\n    text-align: center;\r\n    justify-content: center;\r\n    background: none;\r\n    width: 30px;\r\n    height: 30px;\r\n    color: white;\r\n    border-radius: 50%;\r\n    background: red;\r\n    margin: 37px auto;\r\n    padding: 0;\r\n\r\n    &:hover {\r\n        background: #ff0000;\r\n    }\r\n}\r\n</style>"],"sourceRoot":""}]}