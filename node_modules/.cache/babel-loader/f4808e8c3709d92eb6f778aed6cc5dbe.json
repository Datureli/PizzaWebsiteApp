\n  border: none;\r\n}\r\nheader {\r\n  width: 100%;\r\n  height: 60px;\r\n  background: orange;\r\n  border-radius: 5px;\r\n  display: flex;\r\n  padding: 10px;\r\n\r\n  h1 {\r\n    width: 100%;\r\n    text-align: left;\r\n    margin-left: 60px;\r\n  }\r\n\r\n  .psize {\r\n    width: 100%;\r\n    display: flex;\r\n    text-align: center;\r\n    justify-content: center;\r\n    padding: 10px;\r\n    margin-right: 90px;\r\n\r\n    p {\r\n      margin-left: 17px;\r\n      padding: 0;\r\n    }\r\n  }\r\n}\r\ni {\r\n  pointer-events: none;\r\n}\r\n\r\nh2 {\r\n  font-size: 30px;\r\n  color: #dddddd;\r\n  padding: 5px;\r\n  margin-left: 15px;\r\n}\r\na {\r\n  text-decoration: none;\r\n}\r\np {\r\n  font-size: 20px;\r\n  padding: 0 20px 5px;\r\n  color: rgb(184, 169, 169);\r\n}\r\n\r\n.pizzasize {\r\n  display: flex;\r\n\r\n  p {\r\n    margin-top: 40px;\r\n  }\r\n}\r\n\r\n.pizzaname {\r\n  display: flex;\r\n  margin: 16px 45px 0;\r\n  max-height: 80px;\r\n  line-height: 30px;\r\n}\r\n.pizzaname2 {\r\n  width: 500px;\r\n}\r\nselect {\r\n  width: 115px;\r\n  height: 40px;\r\n  padding: 10px;\r\n  margin: 32px 20px;\r\n  line-height: 25px;\r\n  font-size: 13px;\r\n  background: orange;\r\n  text-align: center;\r\n  justify-content: center;\r\n  font-weight: 600;\r\n\r\n  &:hover {\r\n    background: orangered;\r\n    cursor: pointer;\r\n  }\r\n}\r\n\r\n.minusbutton {\r\n  text-align: center;\r\n  justify-content: center;\r\n  background: none;\r\n  width: 30px;\r\n  height: 30px;\r\n  color: white;\r\n  border-radius: 50%;\r\n  background: red;\r\n  margin: 37px auto;\r\n  padding: 0;\r\n\r\n  &:hover {\r\n    background: #ff0000;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}